<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuActivity">

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_back_btn" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Menu"
        android:textSize="20dp"
        android:fontFamily="@font/helveticanueu"
        android:textColor="@color/dark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="84dp"
        android:fontFamily="@font/helveticanueu"
        android:text="My profile"
        android:textAllCaps="true"
        android:textColor="@color/dark"
        android:textSize="@dimen/client_option_text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/go_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="40dp"
        android:fontFamily="@font/helveticanueu"
        android:text="Settings"
        android:textAllCaps="true"
        android:textColor="@color/dark"
        android:textSize="@dimen/client_option_text"
        app:layout_constraintStart_toStartOf="@+id/textView42"
        app:layout_constraintTop_toBottomOf="@+id/textView42" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/helveticanueu"
        android:text="Logout"
        android:textAllCaps="true"
        android:textColor="@color/dark"
        android:textSize="@dimen/client_option_text"
        app:layout_constraintStart_toStartOf="@+id/go_settings"
        app:layout_constraintTop_toBottomOf="@+id/go_settings" />

    <TextView
        android:id="@+id/go_account_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/helveticanueu"
        android:text="Weekly schedule "
        android:textAllCaps="true"
        android:textColor="@color/dark"
        android:textSize="@dimen/client_option_text"
        app:layout_constraintStart_toStartOf="@+id/textView34"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Billing"
        android:textAllCaps="true"

        android:fontFamily="@font/helveticanueu"
        android:textColor="@color/dark"
        android:textSize="@dimen/client_option_text"
        app:layout_constraintStart_toStartOf="@+id/go_account_info"
        app:layout_constraintTop_toBottomOf="@+id/go_account_info" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:textAllCaps="true"
        android:text="Invite your client"

        android:fontFamily="@font/helveticanueu"
        android:textColor="@color/dark"
        android:textSize="@dimen/client_option_text"
        app:layout_constraintStart_toStartOf="@+id/textView40"
        app:layout_constraintTop_toBottomOf="@+id/textView40" />

    <TextView
        android:id="@+id/textView42"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Payment plan"
        android:textAllCaps="true"
        android:fontFamily="@font/helveticanueu"
        android:textColor="@color/dark"
        android:textSize="@dimen/client_option_text"
        app:layout_constraintStart_toStartOf="@+id/textView41"
        app:layout_constraintTop_toBottomOf="@+id/textView41" />
</androidx.constraintlayout.widget.ConstraintLayout>